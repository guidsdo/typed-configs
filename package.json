  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidsdo/typed-configs.git"
  },
  "keywords": [
    "env",
    "environment",
    "config",
    "typed config",
    "config inheritance",
    "yml",
    "typescript",
    "decorators"
  ],
  "scripts": {
